{
  "expo": {
    "name": "AppCrypto",
    "slug": "AppCrypto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "AppCrypto",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.andresguema.AppCrypto"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d1834088-b62f-4e72-af99-62915afd30b2"
      }
    }
  }
}
